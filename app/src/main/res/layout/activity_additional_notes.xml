<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdditionalNotesActivity">

    <LinearLayout
        android:layout_width="599dp"
        android:layout_height="960dp"
        android:background="#959494"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <EditText
            android:id="@+id/editTextTextMultiLine10"
            android:layout_width="match_parent"
            android:layout_height="8dp"
            android:ems="10"
            android:gravity="start|top"
            android:inputType="textMultiLine" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:text="Additional Notes"
            android:textColor="#000000"
            android:textSize="48sp" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="match_parent"
            android:layout_height="248dp"
            android:text="(Robot design choices, autonomous paths, individual experience, past awards, etc.)"
            android:textColor="#000000"
            android:textSize="20sp" />

        <Button
            android:id="@+id/button8"
            android:layout_width="182dp"
            android:layout_height="wrap_content"
            android:text="Previous Page"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:text="5"
            android:textColor="#000000" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>