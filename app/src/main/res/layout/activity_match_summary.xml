            app:layout_constraintBottom_toBottomOf="@+id/autoActiveFuelValue"
            app:layout_constraintLeft_toRightOf="@+id/autoActiveFuelLabel"
            app:layout_constraintRight_toRightOf="@+id/autoLabel"
            />

