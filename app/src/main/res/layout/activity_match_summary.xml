<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginVertical="20dp"
    tools:context=".matchScouting.MatchScoutingSummaryActivity">

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/additionalNotes"
        android:hint="Additional Notes"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHeight_percent=".25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        >

        <com.google.android.material.textfield.TextInputEditText
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </com.google.android.material.textfield.TextInputLayout>


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/autoLabel"
        android:layout_width="214dp"
        android:layout_height="0dp"
        android:background="#CCD1D1"
        android:minWidth="70dp"
        android:text=" Autonomous"
        android:textColor="@color/cardview_dark_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/teleopLabel"
        app:layout_constraintTop_toBottomOf="@+id/additionalNotes"
        />

    <com.google.android.material.checkbox.MaterialCheckBox
        android:id="@+id/autoLowClimbCheckBox"
        android:layout_width="138dp"
        android:layout_height="25dp"
        android:text="Low Climb"
        android:textColor="@color/cardview_dark_background"
        android:textSize="12dp"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toTopOf="@+id/autoActiveFuelLabel"
        app:layout_constraintEnd_toEndOf="@+id/autoLabel"
        app:layout_constraintStart_toStartOf="@+id/autoLabel"
        app:layout_constraintTop_toTopOf="@+id/autoLabel" />

    <ImageButton
            android:id="@+id/autoActiveFuelDecrement"
            android:layout_width="50dp"
            android:layout_height="25dp"
            android:text="-"
            android:src="@drawable/button_decrement_small"
            android:background="@null"
            app:layout_constraintTop_toTopOf="@+id/autoActiveFuelLabel"
            app:layout_constraintBottom_toBottomOf="@+id/autoActiveFuelValue"
            app:layout_constraintLeft_toLeftOf="@id/autoLabel"
            app:layout_constraintRight_toLeftOf="@id/autoActiveFuelLabel"
            />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/autoActiveFuelLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12dp"
            android:minWidth="70dp"
            android:text="Active Fuel"
            android:textAlignment="center"
            android:textColor="@color/cardview_dark_background"
            app:layout_constraintTop_toBottomOf="@+id/autoLowClimbCheckBox"
            app:layout_constraintBottom_toTopOf="@+id/autoActiveFuelValue"
            app:layout_constraintStart_toStartOf="@+id/autoLabel"
            app:layout_constraintEnd_toEndOf="@+id/autoLabel"
            />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/autoActiveFuelValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="70dp"
            android:text="0"
            android:textAlignment="center"
            android:textColor="@color/cardview_dark_background"
            android:textSize="12dp"
            app:layout_constraintBottom_toTopOf="@+id/autoMissedFuelLabel"
            app:layout_constraintEnd_toEndOf="@+id/autoLabel"
            app:layout_constraintStart_toStartOf="@+id/autoLabel"
            app:layout_constraintTop_toBottomOf="@id/autoActiveFuelLabel" />

        <ImageButton
            android:id="@+id/autoActiveFuelIncrement"
            android:layout_width="50dp"
            android:layout_height="25dp"
            android:text="+"
            android:src="@drawable/button_increment_small"
            android:background="@null"
