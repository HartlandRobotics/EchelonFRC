        />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/final_park"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/blueAlliance"
        app:layout_constraintBottom_toTopOf="@id/net"
        app:layout_constraintTop_toBottomOf="@id/low_hang"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@id/processor"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="5dp"
        android:text="Park"
        />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/summary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Summary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


